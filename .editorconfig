#http://editorconfig.org

root=true

[*]
charset=utf-8 #设置文件字符集为utf-8
indent_style = tab #缩进风格(tab | space)
indent_size = 2 #缩进大小
end_of._line = lf #控制换行类型(lf | cr I crlf)
trim_.trailing.whitespace=true #去除行首的任意空白字符
insert_final_newline=true # 始终在文件末尾插入一个新行
[*.md]#表示仅md文件适用以下规则
max_line_Length=off
trim_.trailing.whitespace= false
